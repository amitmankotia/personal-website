{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "comma-dangle": 0,
        "react/jsx-uses-vars": 1,
        "react/display-name": 1,
        "no-console": 1,
        "prefer-template": 1,
        "no-extra-boolean-cast": 0,
        "no-unexpected-multiline": "warn",
        "import/prefer-default-export": "off",
        "semi": [
            "error",
            "always"
        ],
        "react/jsx-props-no-spreading": "off",
        "no-unused-expressions": 0
    }
}
